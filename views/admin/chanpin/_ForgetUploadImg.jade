.panel-body.panel-uploadChanPinImg
  form.form-horizontal.tasi-form(method='post', action='/admin/reuploadImg', enctype='multipart/form-data')
    .form-group
      label.col-sm-3.control-label 上传图片(选择图片时按住Ctrl多选)
      .col-sm-9
        input.form-control(type='file', name='image', multiple='multiple')
    .form-group
      .col-lg-offset-2.col-lg-10
        input.btn.btn-success.pull-right(type='submit', value='上传图片')
